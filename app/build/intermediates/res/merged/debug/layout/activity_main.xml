<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    android:padding="0dp"
    tools:context="atmconsultoria.estagiariopleno.com.atmconsultoria.MainActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">

    <ImageView
        android:id="@+id/logoId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:src="@drawable/logo" />

    <ImageView
        android:id="@+id/buttonClientesId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonEmpresaId"
        android:layout_alignStart="@+id/buttonEmpresaId"
        android:layout_below="@+id/buttonEmpresaId"
        android:layout_marginTop="19dp"
        android:contentDescription="ignore"
        android:src="@drawable/menu_cliente" />

    <ImageView
        android:id="@+id/buttonContatoId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ButtonServicoId"
        android:layout_alignStart="@+id/ButtonServicoId"
        android:layout_alignTop="@+id/buttonClientesId"
        android:contentDescription="ignore"
        android:src="@drawable/menu_contato" />

    <ImageView
        android:id="@+id/ButtonServicoId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/logoId"
        android:layout_alignRight="@+id/logoId"
        android:layout_alignTop="@+id/buttonEmpresaId"
        android:contentDescription="ignore"
        android:src="@drawable/menu_servico"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/buttonEmpresaId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/logoId"
        android:layout_alignStart="@+id/logoId"
        android:layout_below="@+id/logoId"
        android:layout_marginTop="25dp"
        android:contentDescription="ignore"
        android:src="@drawable/menu_empresa"
        tools:ignore="ContentDescription" />

</android.widget.RelativeLayout>
